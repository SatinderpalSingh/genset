        </div>
    <% end %>

    <%= link_to genexpenses_path, {:class=>"link-color"} do %>
            <div class="icon5"> 
                    <%= icon('inr', class: 'icon') %>
                    <br><br>
            <p class="textIcon" style="text-align:center">Generator Expenses</p>
        </div>
    <% end %>

    <%= link_to thirdparty_path, {:class=>"link-color"} do %>
            <div class="icon6">
                    <%= icon('users', class: 'icon') %>
                    <br><br>
            <p class="textIcon" style="text-align:center">Third Party Providers</p>
        </div>
    <% end %>

    </main>
</section>

